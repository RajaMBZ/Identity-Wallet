{
  "name": "selfkey-identity-wallet",
  "productName": "SelfKey Identity Wallet",
  "version": "0.0.8-beta",
  "description": "The Official SelfKey Identity Wallet for Desktop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelfKeyFoundation/Identity-Wallet.git"
  },
  "keywords": [
    "selfkey",
    "identity",
    "wallet",
    "cryptocurrency",
    "ethereum",
    "erc20"
  ],
  "author": {
    "name": "SelfKey Foundation",
    "email": "info@selfkey.org",
    "url": "https://selfkey.org/"
  },
  "bugs": {
    "url": "https://github.com/SelfKeyFoundation/Identity-Wallet/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "main": "./src/main/main.js",
  "scripts": {
    "dev": "gulp && electron-webpack dev",
    "postinstall": "electron-builder install-app-deps",
    "compile": "gulp && electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "node test/test.js unit",
    "test:e2e": "node test/test.js e2e",
    "publish-build": "yarn compile && electron-builder -p always",
    "precommit": "pretty-quick --staged && gulp deps",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "check-deps": "npm-check -i common"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ledgerhq/hw-transport-node-hid": "4.16.0",
    "@ledgerhq/web3-subprovider": "4.15.0",
    "@sentry/electron": "^0.5.5",
    "@uirouter/angularjs": "1.0.18",
    "angular": "^1.7.2",
    "angular-animate": "^1.7.2",
    "angular-aria": "^1.7.2",
    "angular-local-storage": "0.7.1",
    "angular-localforage": "1.3.8",
    "angular-material": "^1.1.10",
    "angular-messages": "^1.7.2",
    "angular-qrcode": "7.2.0",
    "angular-sanitize": "^1.7.2",
    "angular-zxcvbn": "3.2.2",
    "async": "2.6.1",
    "bignumber.js": "7.2.1",
    "bluebird": "3.5.1",
    "crypto": "1.0.1",
    "decompress": "4.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-redux": "^1.3.1",
    "ethereumjs-tx": "^1.3.6",
    "ethereumjs-util": "5.2.0",
    "fibers": "2.0.2",
    "fs-extra": "6.0.1",
    "google-charts": "1.1.0",
    "is-online": "^7.0.0",
    "keccak": "1.4.0",
    "knex": "^0.15.0",
    "ledgerco": "1.2.1",
    "lodash": "^4.17.10",
    "mime-types": "2.1.18",
    "mv": "2.1.1",
    "node-fetch": "^2.1.2",
    "objection": "^1.1.10",
    "promise-timeout": "1.3.0",
    "qrcode-generator": "1.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react2angular": "^4.0.4",
    "redux": "^3.7.2",
    "request": "2.87.0",
    "secp256k1": "3.5.0",
    "selfkey-ui": "https://github.com/SelfKeyFoundation/selfkey-ui.git",
    "sjcl": "1.0.7",
    "source-map-support": "^0.5.6",
    "sqlite3": "^4.0.0",
    "striptags": "^3.1.1",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.34",
    "web3-provider-engine": "14.0.6",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.51",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@sentry/cli": "^1.33.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "delay": "^3.0.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-chromedriver": "^2.0.0",
    "electron-download": "^4.1.0",
    "electron-log": "^2.2.16",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^2.23.3",
    "electron-webpack": "^2.1.2",
    "electron-webpack-eslint": "^2.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^2.2.1",
    "gulp-npm-check": "^1.1.0",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.1",
    "gulp-watch": "^5.0.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "knex-migrate": "^1.6.1",
    "mocha": "^5.2.0",
    "npm-check": "^5.7.1",
    "pretty-quick": "^1.6.0",
    "run-sequence": "^2.2.1",
    "spectron": "^3.8.0",
    "webpack": "^4.16.0",
    "webpack-build-notifier": "^0.1.28",
    "webpack-sources": "1.1.0"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "migrations": {
    "version": "20180508093508"
  },
  "seeds": {
    "version": "init"
  },
  "build": {
    "appId": "org.selfkey.wallet",
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "dist/migrations",
      "dist/seeds",
      "dist/assets",
      "dist/utils"
    ]
  },
  "electronWebpack": {
    "whiteListedModules": [
      "source-map-support"
    ],
    "main": {
      "extraEntries": [
        "@/preload.js"
      ],
      "webpackConfig": "webpack.main.additions.js"
    },
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
