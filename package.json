		"buildDependenciesFromSource": true,
		"generateUpdatesFilesForAllChannels": true,
		"files": [
			"dist/main",
			"dist/common"
		]
	},
	"electronWebpack": {
		"whiteListedModules": [
			"source-map-support"
		],
		"main": {
			"extraEntries": [
				"@/preload.js"
			],
			"webpackConfig": "webpack/main.additions.js"
